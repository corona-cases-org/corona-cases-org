{
  "name": "adonis-fullstack-app",
  "version": "4.1.0",
  "adonis-version": "4.1.0",
  "description": "The fullstack application boilerplate for Adonisjs",
  "main": null,
  "scripts": {
    "start": "node server.js",
    "test": "node ace test",
    "build": "rm -rf public/assets; broccoli build public/assets",
    "heroku-postbuild": "npm run build && (echo; echo \"SOURCE_VERSION=$SOURCE_VERSION\") >> .env",
    "build:favicon": "curl -s https://upload.wikimedia.org/wikipedia/commons/8/82/SARS-CoV-2_without_background.png | convert - -resize 256x256 -background transparent -gravity center -extent 256x256 -define icon:auto-resize=\"32,16\" public/favicon.ico",
    "build:data": "curl -s -o data/locations.json https://coronadatascraper.com/locations.json && curl -s -o data/timeseries.json https://coronadatascraper.com/timeseries.json"
  },
  "engines": {
    "node": "14.4.0"
  },
  "keywords": [
    "adonisjs",
    "adonis-app"
  ],
  "author": "",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "@adonisjs/ace": "^5.0.8",
    "@adonisjs/auth": "^3.0.7",
    "@adonisjs/bodyparser": "^2.0.5",
    "@adonisjs/cors": "^1.0.7",
    "@adonisjs/fold": "^4.0.9",
    "@adonisjs/framework": "^5.0.9",
    "@adonisjs/ignitor": "^2.0.8",
    "@adonisjs/lucid": "^6.1.3",
    "@adonisjs/session": "^1.0.27",
    "@adonisjs/shield": "^1.0.8",
    "@babel/core": "^7.10.2",
    "@babel/plugin-proposal-class-properties": "^7.10.1",
    "@babel/plugin-proposal-object-rest-spread": "^7.10.1",
    "@babel/plugin-transform-react-jsx": "^7.10.3",
    "@babel/preset-env": "^7.10.2",
    "@rollup/plugin-babel": "^5.0.2",
    "@rollup/plugin-commonjs": "^12.0.0",
    "@rollup/plugin-json": "^4.0.3",
    "@rollup/plugin-node-resolve": "^8.0.0",
    "@rollup/plugin-replace": "^2.3.3",
    "broccoli": "^3.4.2",
    "broccoli-asset-rev": "^3.0.0",
    "broccoli-caching-writer": "^3.0.3",
    "broccoli-clean-css": "^2.0.1",
    "broccoli-funnel": "^3.0.3",
    "broccoli-merge-trees": "^4.2.0",
    "broccoli-pug2": "^0.2.1",
    "broccoli-rollup": "^4.1.1",
    "broccoli-sass-source-maps": "^4.0.0",
    "broccoli-uglify-sourcemap": "^3.2.0",
    "glob": "^7.1.6",
    "highcharts": "^8.1.0",
    "highcharts-react-official": "^3.0.0",
    "lodash": "^4.17.15",
    "mkdirp": "^1.0.4",
    "moment": "^2.26.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "devDependencies": {
    "@adonisjs/cli": "^4.0.12",
    "broccoli-cli": "^1.0.0",
    "nodemon": "^2.0.4"
  },
  "autoload": {
    "App": "./app"
  }
}
